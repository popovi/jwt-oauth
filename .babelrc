{
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/transform-runtime",
      "@babel/plugin-transform-react-jsx-source",
      "@babel/plugin-proposal-class-properties",
      [
        "babel-plugin-styled-components", {
          "minify": false,
          "transpileTemplateLiterals": false
        }
      ]
    ]
  }